      <C_ell_beta>${C_ell_beta}</C_ell_beta>
      <C_ell_p>${C_ell_p}</C_ell_p>
      <C_ell_q>${C_ell_q}</C_ell_q>
      <C_ell_r>${C_ell_r}</C_ell_r>
