      <C_L_delta_r>${C_L_delta_r}</C_L_delta_r>
      <C_D_O>${C_D_O}</C_D_O>
      <C_D_alpha>${C_D_alpha}</C_D_alpha>
      <C_D_beta>${C_D_beta}</C_D_beta>
      <C_D_p>${C_D_p}</C_D_p>
      <C_D_q>${C_D_q}</C_D_q>
      <C_D_r>${C_D_r}</C_D_r>
