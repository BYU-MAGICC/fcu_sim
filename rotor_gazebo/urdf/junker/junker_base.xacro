  <!-- Property Blocks -->
  <xacro:property name="inertia">
    <inertia ixx="0.8244" ixy="0.0" ixz=".1204" iyy="1.135" iyz="0.0" izz="1.759" />
  </xacro:property>

    <xacro:property name="empty_inertia">
    <inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.001" />
  </xacro:property>
    
  <!-- Instantiate Links and Joints -->
  <link name="${namespace}/base_link">
    <inertial>
      <mass value="${mass}" />
      <origin xyz="0 0 0" />
      <xacro:insert_block name="empty_inertia" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="1.570796 0 1.570796" />
      <geometry>
        <xacro:if value="${use_mesh_file}">
          <mesh filename="${mesh_file}" scale="0.001 0.001 0.001" />
        </xacro:if>
        <xacro:unless value="${use_mesh_file}">
          <box size="${body_length} ${body_width} ${body_height}"/> <!-- [m] [m] [m] -->
        </xacro:unless>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.570796 0 1.570796" />
      <geometry>
        <box size="${body_length} ${body_width} ${body_height}" />
      </geometry>
    </collision>    
  </link>

  <!-- Add Aerodynamics Link -->
  <link name="${namespace}/aerodynamics_link">
    <inertial>
      <mass value="${mass}" />
      <origin xyz="0 0 0" />
      <xacro:insert_block name="empty_inertia" />
    </inertial>
  </link>

  <!-- Add Aerodynamics Joint -->
  <joint name="${namespace}/aerodynamics_joint" type="revolute">
    <parent link="${namespace}/base_link"/>
    <child link="${namespace}/aerodynamics_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <limit upper="0" lower="0" effort="0" velocity="0" />
  </joint>


  <!-- Add Aerodynamics -->
  <gazebo>
    <plugin filename="librotor_gazebo_aircraft_forces_and_moments_plugin.so" name="aircraft_forces_and_moments_plugin">
      <namespace>${namespace}</namespace>
      <linkName>${namespace}/aerodynamics_link</linkName>
      <windSpeedTopic>${wind_speed_topic}</windSpeedTopic>
      <commandTopic>${command_topic}</commandTopic>

      <mass>${mass}</mass>
      <Jx>${Jx}</Jx>
      <Jy>${Jy}</Jy>
      <Jz>${Jz}</Jz>
      <Jxz>${Jxz}</Jxz>
      <rho>${rho}</rho>
      <wing_s>${wing_s}</wing_s>
      <wing_b>${wing_b}</wing_b>
      <wing_c>${wing_c}</wing_c>
      <wing_M>${wing_M}</wing_M>
      <wing_epsilon>${wing_epsilon}</wing_epsilon>
      <wing_alpha0>${wing_alpha0}</wing_alpha0>
      <k_motor>${k_motor}</k_motor>
      <k_T_P>${k_T_P}</k_T_P>
      <k_Omega>${k_Omega}</k_Omega>
      <prop_e>${prop_e}</prop_e>
      <prop_S>${prop_S}</prop_S>
      <prop_C>${prop_C}</prop_C>
      <C_L_O>${C_L_O}</C_L_O>
